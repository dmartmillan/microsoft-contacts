<h1>Contacts</h1>
<table class="table">
    <thead class="thead-light">
    <th scope="col">First Name</th>
    <th scope="col">Last name</th>
    <th scope="col">Mobile phone</th>
    </thead>
    <tbody>
    {{#each contacts}}
        <tr>
            <td>{{this.givenName}}</td>
            <td>{{this.surname}}</td>
            <td>{{this.mobilePhone}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
